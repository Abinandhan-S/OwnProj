{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "-webkit-tap-highlight-color",
          "-ms-text-size-adjust",
          "-webkit-font-feature-settings"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "typescript-config/consistent-casing": "off",
    "typescript-config/strict": "off"
  }
}